{
  "name": "rains-plastics-llc",
  "version": "0.0.1",
  "description": "The Official Website of Rains Plastics LLC",
  "productName": "Rains Plastic Website",
  "author": "Tajohnson245 <tajohnson245@outlook.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "start": "quasar serve dist/spa --history",
    "vercel-build": "npm install && npm run build && mkdir -p .vercel/output/static && cp -r dist/spa/* .vercel/output/static/ && cp -r src/assets .vercel/output/static/",
    "vercel-dev": "npm install && npm run dev",
    "api": "node server.cjs",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\""
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@vueuse/motion": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "nodemailer": "^6.9.9",
    "quasar": "^2.16.0",
    "sweetalert2": "^11.19.1",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
